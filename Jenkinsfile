pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    

        stage('build') {
            steps {
                echo 'Building'
            }
        }
    
    
     
        stage('Deploy') {
            steps {
                echo 'Deploying hello'
            }
        }

    
    
        stage('Test') {
            steps {
                echo 'Testing hello'
            }
        }

    
     
        stage('release') {
            steps {
                echo 'releasing'
            }
        }
    
    }
}
